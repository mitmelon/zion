{
  "name": "mitmelon/zionxmemory",
  "description": "Production-ready, research-backed, enterprise-grade adaptive memory system that maintains the core principle: ZionXMemory is memory infrastructure, not an agent controller.",
  "license": "MIT",
  "authors": [
    {
      "email": "manomitehq@gmail.com",
      "name": "Manomite Limited"
    }
  ],
  "autoload": {
      "psr-4": {
          "ZionXMemory\\": "src/"
      }
  },
  "config": {
    "process-timeout": 600,
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "google-gemini-php/client": "^2.7",
    "guzzlehttp/guzzle": "^7.10",
    "psr/log": "^3.0"
  },
  "type": "library"
}
